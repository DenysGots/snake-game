{
  "name": "snake_game",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "node": "12.17.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DenysGots/snake-game.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "ng lint",
    "build": "ng build",
    "build:ssr": "ng build --configuration=production && ng run universal-starter-v9:server:production",
    "build:ssr:local": "ng build --configuration=local && ng run universal-starter-v9:server",
    "serve:ssr": "node dist/universal-starter-v9/server/main.js",
    "dev:ssr": "ng run universal-starter-v9:serve-ssr",
    "dev:local": "nodemon --delay 5 --watch src/ -e ts,html,scss --exec \"npm run build:ssr:local && npm run serve:ssr --configuration=local\"",
    "prod": "prettier --write \"src/**/*.{ts,html,css}\"",
    "prebuild:ssr": "ngcc",
    "prerender": "ng run universal-starter-v9:prerender",
    "postinstall": "ngcc",
    "heroku-postbuild": "npm run build:ssr"
  },
  "dependencies": {
    "@angular/animations": "10.0.7",
    "@angular/cdk": "^10.1.1",
    "@angular/common": "10.0.7",
    "@angular/compiler": "10.0.7",
    "@angular/core": "11.0.5",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "10.0.7",
    "@angular/material": "10.1.1",
    "@angular/platform-browser": "10.0.7",
    "@angular/platform-browser-dynamic": "10.0.7",
    "@angular/platform-server": "10.0.7",
    "@angular/router": "10.0.7",
    "@nestjs/cli": "^7.4.1",
    "@nestjs/common": "^7.4.2",
    "@nestjs/core": "^7.4.2",
    "@nestjs/microservices": "^7.4.2",
    "@nestjs/ng-universal": "^3.2.0",
    "@nestjs/platform-express": "^7.4.2",
    "@ngrx/store": "^9.2.0",
    "@nguniversal/common": "^10.0.1",
    "@nguniversal/express-engine": "^10.0.1",
    "bootstrap": "^4.5.0",
    "classlist.js": "^1.1.20150312",
    "compression": "^1.7.4",
    "concurrently": "^5.2.0",
    "core-js": "^3.6.5",
    "lodash": "^4.17.19",
    "rxjs": "^6.6.2",
    "sass": "^1.26.10",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^2.0.0",
    "webpack-cli": "^3.3.12",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.4",
    "@angular/cli": "10.0.4",
    "@angular/compiler-cli": "10.0.7",
    "@angular/language-service": "10.0.7",
    "@nguniversal/builders": "10.0.1",
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "codelyzer": "^6.0.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.5.0",
    "eslint-plugin-import": "^2.22.0",
    "express": "^4.17.1",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "reflect-metadata": "^0.1.13",
    "ts-loader": "^8.0.1",
    "ts-node": "~8.10.2",
    "tslint": "^6.1.2",
    "typescript": "3.9.7"
  }
